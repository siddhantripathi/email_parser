{
    "version": 2,
    "builds": [
        {
            "src": "email_parser/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "email_parser/main.py"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb-uri"
    }
} 