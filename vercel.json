{
    "version": 2,
    "builds": [
        {
            "src": "email_parser/main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "email_parser/main.py"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb-uri"
    }
} 